AAA;AACA;AACA,YAFA,oBAEA;AACA,WAAA,OAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,IAAA,EAAA;AACA,WAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IAAA,EAAA;AACA;AALA,GAZA;AAmBA,EAAA,OAnBA,qBAmBA;AAAA;;AACA,SAAA,KAAA,CAAA,KAAA,eAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,aAAA,QAAA,CAAA,IAAA,EAAA;AACA,KAHA,EAIA,IAJA,CAIA,UAAA,IAAA,EAAA;AACA,MAAA,KAAA,CAAA,iBAAA,CAAA,CAAA,IAAA;AACA,KANA;AAOA,SAAA,YAAA,CAAA,KAAA,IAAA;AACA,GA5BA;AA6BA,EAAA,QAAA,oBACA,UAAA,CAAA,CAAA,cAAA,CAAA,CADA,CA7BA;AAgCA,EAAA,OAAA,kCACA,UAAA,CAAA,CAAA,iBAAA,EAAA,YAAA,CAAA,CADA;AAEA,IAAA,MAFA,oBAEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,IAAA,EAAA,CAAA;AAAA,UAAA,OAAA,EAAA,KAAA,OAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA;AAAA,OAAA;AACA,KAJA;AAKA,IAAA,YAAA,EAAA,sBAAA,IAAA,EAAA;AACA;AACA,UAAA,IAAA,CAAA,KAAA,EAAA;AACA;AACA,QAAA,QAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,OAHA,MAGA;AACA,QAAA,QAAA,CAAA,KAAA,GAAA,aAAA;AACA,OAPA,CAQA;;;AACA,UAAA,IAAA,CAAA,WAAA,EAAA;AACA;AACA,QAAA,QAAA,CAAA,aAAA,CAAA,0BAAA,EAAA,YAAA,CAAA,SAAA,EAAA,IAAA,CAAA,WAAA;AACA,OAHA,MAGA;AACA,QAAA,QAAA,CAAA,aAAA,CAAA,0BAAA,EAAA,YAAA,CAAA,SAAA,EAAA,oBAAA;AACA;AACA;AApBA;AAhCA,CAAA","sourcesContent":["<template>\n  <header>\n    <div id=\"title\">\n      <router-link id=\"rl\" :to=\"{name: 'posts'}\">\n        <div v-if=\"titleimage\"><img :src=\"titleimage\"/></div>\n        <div v-else><img src=\"@/assets/title.png\"/></div>\n      </router-link>\n    </div>\n    <div id=\"header\">\n      <h1>\n        <!-- <router-link :to=\"{name: 'posts'}\">Monochro-me</router-link> -->\n        <router-link :to=\"{name: 'profile'}\">Profile</router-link>\n      </h1>\n      <div id=\"form\">\n        <input type=\"text\" placeholder=\"Search\" class=\"text\" v-model=\"keyword\" @change=\"search\">\n        <div class=\"selectWrap\">\n          <select class=\"select\" v-model=\"selected\" @change=\"search\">\n            <option value=\"\" :key=\"-1\">Category</option>\n            <option v-for=\"category of categoryList\" :value=\"category.id\" :key=\"category.id\">{{category.name}}</option>\n          </select>\n        </div>\n      </div>\n    </div>\n  </header>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex'\nimport { UPDATE_CATEGORIES, UPDATE_POSTS } from '@/store/mutation-types'\n\nexport default {\n  name: 'site-header',\n  data () {\n    // URL内のGETパラメータを参照\n    return {\n      keyword: this.$route.query.keyword || '',\n      selected: this.$route.query.category || ''\n    }\n  },\n  props: {\n    titleimage: null\n  },\n  watch: {\n    // サイトタイトルクリックでトップページに戻る\n    '$route' () {\n      this.keyword = this.$route.query.keyword || ''\n      this.selected = this.$route.query.category || ''\n    }\n  },\n  created () {\n    this.$http(this.$httpCategories)\n      .then(response => {\n        return response.json()\n      })\n      .then(data => {\n        this[UPDATE_CATEGORIES](data)\n      })\n    this.setTitleDesc(this.site)\n  },\n  computed: {\n    ...mapGetters(['categoryList'])\n  },\n  methods: {\n    ...mapActions([UPDATE_CATEGORIES, UPDATE_POSTS]),\n    search () {\n      this.$router.push({ name: 'posts', query: { page: 1, keyword: this.keyword, category: this.selected } })\n    },\n    setTitleDesc: function (site) {\n      // titleを設定する処理\n      if (site.title) {\n        // var setTitle = routeInstance.meta.title + ' | サイト名など(任意)';\n        document.title = site.title\n      } else {\n        document.title = 'monochro-me'\n      }\n      // メタタグのdescription設定処理\n      if (site.description) {\n        // var setDesc = routeInstance.meta.desc + ' | 共通表示名';\n        document.querySelector(\"meta[name='description']\").setAttribute('content', site.description)\n      } else {\n        document.querySelector(\"meta[name='description']\").setAttribute('content', '数学とLaTeXと, 時々, オトン')\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\nheader {\n  /* height: 50px; */\n  display: grid;\n  /* grid-template-columns: 20px 1fr 20px; */\n  grid-template-rows: 1fr;\n  margin-bottom: 80px;\n}\n/*\nheader > * {\n  grid-row: 1;\n  grid-column: 2;\n}\n*/\n#title {\n  background-color: #272822;\n  display: grid;\n  grid-template-columns: 20px 1fr 20px;\n}\n\n#rl {\n  grid-column: 2;\n  margin: 0 0 0 auto;\n}\n\n#rl > img {\n  width: 100%;\n  height: auto;\n}\n\n#header {\n  height: 50px;\n  background-color: #f8f8f2;\n  display: grid;\n  grid-template-columns: 20px 1fr 20px;\n}\n\nh1 {\n  grid-column: 2;\n  justify-self: start;\n  align-self: center;\n  font-size: 18px;\n  font-weight: normal;\n}\n\nh1 > a {\n  color: #272822;\n  text-decoration: none;\n}\n\n#form {\n  grid-column: 3;\n  justify-self: end;\n  align-self: center;\n  display: none;\n}\n\n.text {\n  border-bottom: solid 1px #444;\n  border-right: none;\n  border-top: none;\n  border-left: none;\n  background-color: transparent;\n  color: #272822;\n  width: 200px;\n  margin-left: 20px;\n  padding-left: 6px;\n  padding-bottom: 1px;\n  font-family: fot-tsukuardgothic-std, sans-serif;\n}\n\n.selectWrap {\n  margin-left: 20px;\n  width: 150px;\n  position: relative;\n  display: inline-block;\n}\n\n.selectWrap::after {\n  content: '';\n  width: 6px;\n  height: 6px;\n  border: 0;\n  border-bottom: solid 2px #444;\n  border-right: solid 2px #444;\n  -ms-transform: rotate(45deg);\n  -webkit-transform: rotate(45deg);\n  transform: rotate(45deg);\n  position: absolute;\n  top: 50%;\n  right: 10px;\n  margin-top: -4px;\n}\n\n.select {\n  appearance: none;\n  border-bottom: solid 1px #444;\n  border-right: none;\n  border-top: none;\n  border-left: none;\n  background-color: transparent;\n  color: #000;\n  width: 100%;\n  font-family: fot-tsukuardgothic-std, sans-serif;\n}\n\n::placeholder {\n  color: #000;\n  opacity: 1;\n  font-family: fot-tsukuardgothic-std, sans-serif;\n}\n\n@media (min-width: 768px) {\n  /* header {\n    grid-template-columns: 1fr 700px 1fr;\n  } */\n\n  #title {\n    grid-template-columns: 1fr 700px 1fr;\n  }\n\n  #rl {\n    grid-column: 2;\n    height: 150px;\n  }\n\n  #header {\n    grid-template-columns: 1fr 100px 600px 1fr;\n    height: 50px;\n  }\n\n  #form {\n    display: block;\n  }\n}\n\n@media (min-width: 1024px) {\n  /* header {\n    grid-template-columns: 1fr 980px 1fr;\n  } */\n\n  #title {\n    grid-template-columns: 1fr 980px 1fr;\n  }\n\n  #header {\n    grid-template-columns: 1fr 100px 880px 1fr;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}