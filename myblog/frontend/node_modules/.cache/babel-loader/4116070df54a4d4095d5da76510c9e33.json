BA,IApBA,EAoBA;AACA,IAAA,IAAA,CAAA,UAAA,SAAA,EAAA;AACA,UAAA,IAAA,CAAA,IAAA,EAAA;AACA,QAAA,SAAA,CAAA,SAAA,GAAA,IAAA;AACA;AACA,KAJA,CAAA;AAKA,GA1BA;AA2BA,EAAA,OA3BA,qBA2BA;AACA,QAAA,OAAA,CAAA,OAAA,EAAA;AACA,MAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA;AACA;AACA,QAAA,UAAA,EAAA,CAAA,YAAA,CAFA;AAGA,QAAA,GAAA,EAAA,CAAA,WAAA,EAAA,iBAAA,CAHA;AAIA,QAAA,OAAA,EAAA;AACA,UAAA,UAAA,EAAA,CAAA,CAAA,GAAA,EAAA,GAAA,CAAA,EAAA,CAAA,KAAA,EAAA,KAAA,CAAA,CADA;AAEA,UAAA,WAAA,EAAA,CAAA,CAAA,IAAA,EAAA,IAAA,CAAA,EAAA,CAAA,KAAA,EAAA,KAAA,CAAA,CAFA;AAGA,UAAA,cAAA,EAAA;AAHA,SAJA;AASA,oBAAA;AAAA,UAAA,KAAA,EAAA,CAAA,KAAA;AAAA;AATA,OAAA;AAWA;AACA,GAzCA;AA0CA,EAAA,OA1CA,qBA0CA;AAAA;;AACA,SAAA,KAAA,WAAA,KAAA,UAAA,SAAA,KAAA,EAAA,QAAA;AAAA,MAAA,WAAA,EAAA;AAAA,KAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,aAAA,QAAA,CAAA,IAAA,EAAA;AACA,KAHA,EAIA,IAJA,CAIA,UAAA,IAAA,EAAA;AACA,MAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,MAAA,QAAA,CAAA,KAAA,aAAA,IAAA,CAAA,KAAA;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,0BAAA,EAAA,YAAA,CAAA,SAAA,EAAA,IAAA,CAAA,SAAA;;AACA,MAAA,KAAA,CAAA,SAAA,CAAA;AAAA,eAAA,KAAA,CAAA,OAAA,EAAA;AAAA,OAAA;AACA,KATA;AAUA,GArDA;AAsDA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,UAAA,KAAA,SAAA,EAAA;AACA,aAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA;AACA,KAPA;AAQA,IAAA,SARA,uBAQA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA;AACA,QAAA,GAAA,EAAA,CADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAAA;AAIA,KAbA;AAcA,IAAA,OAdA,qBAcA;AACA,UAAA,QAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,aAAA,CAAA,SAAA,CAAA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,SAAA,CAAA,IAAA,CAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,WAAA,CAAA,QAAA;AACA;AAlBA;AAtDA,CAAA","sourcesContent":["<template>\n  <article :key=\"id\" class=\"container\" v-if=\"post\">\n    <header>\n      <nav id=\"back\"><a @click=\"goBack\" title=\"前ページへ戻る\"><img src=\"@/assets/back.png\"></a></nav>\n      <h1 class=\"post-title\">{{post.title}}</h1>\n      <p class=\"post-category\" :style=\"{'color': post.category.color}\">{{post.category.name}}</p>\n    </header>\n    <div id=\"main\">\n      <nav id=\"toc\" ref=\"toc\"></nav>\n      <div id=\"post-main\" ref=\"text\" v-html=\"post.main_text\"></div>\n    </div>\n    <hr class=\"divider\">\n    <p>$$x^2$$</p>\n    <nav id=\"top\"><a @click=\"scrollTop\" title=\"一番上まで戻る\"><img src=\"@/assets/ue.png\"></a></nav>\n  </article>\n</template>\n\n<script>\nexport default {\n  name: 'post',\n  props: {\n    id: { type: Number }\n  },\n  head () {\n    return {\n      script: [\n        {\n          src: 'https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.2/MathJax.js?config=TeX-AMS-MML_SVG'\n        }\n      ]\n    }\n  },\n  data () {\n    return {\n      post: null,\n      hasBefore: false\n    }\n  },\n  beforeRouteEnter (to, from, next) {\n    next(component => {\n      if (from.name) {\n        component.hasBefore = true\n      }\n    })\n  },\n  created () {\n    if (process.browser) {\n      window.MathJax.Hub.Config({\n        // MathJaxの設定\n        extensions: ['tex2jax.js'],\n        jax: ['input/TeX', 'output/HTML-CSS'],\n        tex2jax: {\n          inlineMath: [['$', '$'], ['\\\\(', '\\\\)']],\n          displayMath: [['$$', '$$'], ['\\\\[', '\\\\]']],\n          processEscapes: true\n        },\n        'HTML-CSS': { fonts: ['TeX'] }\n      });\n    }\n  },\n  mounted () {\n    this.$http(`${this.$httpPosts}${this.id}/`, { credentials: 'include' })\n      .then(response => {\n        return response.json()\n      })\n      .then(data => {\n        this.post = data\n        document.title = `${data.title} - Monochro-me`\n        document.querySelector('meta[name=\"description\"]').setAttribute('content', data.lead_text)\n        this.$nextTick(() => this.moveToc())\n      })\n  },\n  methods: {\n    goBack () {\n      if (this.hasBefore) {\n        this.$router.go(-1)\n      } else {\n        this.$router.push({ name: 'posts' })\n      }\n    },\n    scrollTop () {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      })\n    },\n    moveToc () {\n      const innerToc = this.$refs.text.querySelector('div.toc')\n      const cloneToc = innerToc.cloneNode(true)\n      this.$refs.toc.appendChild(cloneToc)\n    }\n  }\n}\n</script>\n\n<style scoped>\nheader {\n  margin-bottom: 80px;\n}\n\n#back {\n  margin-bottom: 80px;\n}\n\n#back a {\n  cursor: pointer;\n  width: 44px;\n  display: inline-block;\n}\n\n#top a {\n  cursor: pointer;\n  color: #999;\n  display: inline-block;\n  width: 44px;\n}\n\n.post-category {\n  font-size: 20px;\n}\n\n.post-title {\n  color: #fff;\n  font-weight: bold;\n  font-size: 28px;\n}\n\n.divider {\n  margin-top: 80px;\n  margin-bottom: 80px;\n  width: 100%;\n  height: 1px;\n  border: none;\n  background-color: #ccc;\n}\n\n#post-main {\n  color: #fff;\n  width: 100%;\n  line-height: 2;\n}\n\n#main >>> div.toc a {\n  font-size: 12px;\n  color: #666666;\n  text-decoration: none;\n}\n\n#post-main >>> > * {\n  margin-bottom: 1.5em;\n}\n\n#post-main >>> div.toc + * {\n  margin-top: 0;\n}\n\n#post-main >>> > h2 {\n  font-weight: bold;\n  font-size: 20px;\n  line-height: 1.5;\n  margin-top: 50px;\n  margin-bottom: 21px;\n}\n\n#post-main >>> img {\n  max-width: 100%;\n  height: auto;\n  box-shadow: 0 0 5px #ccc;\n}\n\n#post-main >>> strong {\n  background-color: yellow;\n}\n\n#post-main >>> div.toc {\n  display: none;\n}\n\n#post-main >>> p {\n  text-indent: 1em;\n}\n\n#toc {\n  display: none;\n}\n\n@media (min-width: 768px) {\n  #post-main {\n    width: 650px;\n  }\n}\n\n@media (min-width: 1024px) {\n  #main {\n    display: grid;\n    grid-template-columns: 150px 650px;\n    column-gap: 50px;\n  }\n\n  #toc {\n    grid-column: 1;\n    display: block;\n    align-self: start;\n    position: sticky;\n    top: 20px;\n  }\n\n  #post-main {\n    grid-column: 2;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}