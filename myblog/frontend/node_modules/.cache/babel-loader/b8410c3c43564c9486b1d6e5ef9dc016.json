EAAA,KAAA,EAAA;AACA,IAAA,EAAA,EAAA;AAAA,MAAA,IAAA,EAAA;AAAA;AADA,GAFA;AAKA,EAAA,IALA,kBAKA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,IADA;AAEA,MAAA,SAAA,EAAA;AAFA,KAAA;AAIA,GAVA;AAWA,EAAA,gBAXA,4BAWA,EAXA,EAWA,IAXA,EAWA,IAXA,EAWA;AACA,IAAA,IAAA,CAAA,UAAA,SAAA,EAAA;AACA,UAAA,IAAA,CAAA,IAAA,EAAA;AACA,QAAA,SAAA,CAAA,SAAA,GAAA,IAAA;AACA;AACA,KAJA,CAAA;AAKA,GAjBA;AAkBA,EAAA,OAlBA,qBAkBA;AAAA;;AACA,SAAA,aAAA;AACA,SAAA,KAAA,WAAA,KAAA,UAAA,SAAA,KAAA,EAAA,QAAA;AAAA,MAAA,WAAA,EAAA;AAAA,KAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,aAAA,QAAA,CAAA,IAAA,EAAA;AACA,KAHA,EAIA,IAJA,CAIA,UAAA,IAAA,EAAA;AACA,MAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,MAAA,QAAA,CAAA,KAAA,aAAA,IAAA,CAAA,KAAA;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,0BAAA,EAAA,YAAA,CAAA,SAAA,EAAA,IAAA,CAAA,SAAA;;AACA,MAAA,KAAA,CAAA,SAAA,CAAA;AAAA,eAAA,KAAA,CAAA,OAAA,EAAA;AAAA,OAAA;AACA,KATA;AAUA,GA9BA;AA+BA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,UAAA,KAAA,SAAA,EAAA;AACA,aAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA;AACA,KAPA;AAQA,IAAA,SARA,uBAQA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA;AACA,QAAA,GAAA,EAAA,CADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAAA;AAIA,KAbA;AAcA,IAAA,OAdA,qBAcA;AACA,UAAA,QAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,aAAA,CAAA,SAAA,CAAA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,SAAA,CAAA,IAAA,CAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,WAAA,CAAA,QAAA;AACA,KAlBA;AAmBA,IAAA,aAnBA,2BAmBA;AACA,UAAA,MAAA,CAAA,OAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA;AACA,UAAA,GAAA,EAAA;AAAA,YAAA,eAAA,EAAA;AAAA,cAAA,UAAA,EAAA;AAAA;AAAA,WADA;AAEA,UAAA,OAAA,EAAA;AACA,YAAA,UAAA,EAAA,CACA,CAAA,GAAA,EAAA,GAAA,CADA,EAEA,CAAA,KAAA,EAAA,KAAA,CAFA,CADA;AAKA,YAAA,cAAA,EAAA;AALA,WAFA;AASA,sBAAA;AAAA,YAAA,eAAA,EAAA;AAAA,WATA;AAUA,UAAA,YAAA,EAAA,QAVA;AAWA,UAAA,aAAA,EAAA;AAXA,SAAA;AAaA,QAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,CAAA,SAAA,EAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA;AACA;AACA;AApCA;AA/BA,CAAA","sourcesContent":["<template>\n  <article :key=\"id\" class=\"container\" v-if=\"post\">\n    <header>\n      <nav id=\"back\"><a @click=\"goBack\" title=\"前ページへ戻る\"><img src=\"@/assets/back.png\"></a></nav>\n      <h1 class=\"post-title\">{{post.title}}</h1>\n      <p class=\"post-category\" :style=\"{'color': post.category.color}\">{{post.category.name}}</p>\n    </header>\n    <div id=\"main\">\n      <nav id=\"toc\" ref=\"toc\"></nav>\n      <div id=\"post-main\" ref=\"text\" v-html=\"post.main_text\"></div>\n    </div>\n    <hr class=\"divider\">\n    <nav id=\"top\"><a @click=\"scrollTop\" title=\"一番上まで戻る\"><img src=\"@/assets/ue.png\"></a></nav>\n  </article>\n</template>\n\n<script>\n// import { VueMathjax } from 'vue-mathjax'\n\nexport default {\n  name: 'post',\n  props: {\n    id: { type: Number }\n  },\n  data () {\n    return {\n      post: null,\n      hasBefore: false\n    }\n  },\n  beforeRouteEnter (to, from, next) {\n    next(component => {\n      if (from.name) {\n        component.hasBefore = true\n      }\n    })\n  },\n  mounted () {\n    this.renderMathJax()\n    this.$http(`${this.$httpPosts}${this.id}/`, { credentials: 'include' })\n      .then(response => {\n        return response.json()\n      })\n      .then(data => {\n        this.post = data\n        document.title = `${data.title} - Monochro-me`\n        document.querySelector('meta[name=\"description\"]').setAttribute('content', data.lead_text)\n        this.$nextTick(() => this.moveToc())\n      })\n  },\n  methods: {\n    goBack () {\n      if (this.hasBefore) {\n        this.$router.go(-1)\n      } else {\n        this.$router.push({ name: 'posts' })\n      }\n    },\n    scrollTop () {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      })\n    },\n    moveToc () {\n      const innerToc = this.$refs.text.querySelector('div.toc')\n      const cloneToc = innerToc.cloneNode(true)\n      this.$refs.toc.appendChild(cloneToc)\n    },\n    renderMathJax () {\n      if (window.MathJax) {\n        window.MathJax.Hub.Config({\n          TeX: { equationNumbers: { autoNumber: 'AMS' } },\n          tex2jax: {\n            inlineMath: [\n              ['$', '$'],\n              ['\\\\(', '\\\\)']\n            ],\n            processEscapes: true\n          },\n          'HTML-CSS': { matchFontHeight: false },\n          displayAlign: 'center',\n          displayIndent: '2em'\n        })\n        window.MathJax.Hub.Queue(['Typeset', window.MathJax.Hub])\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\nheader {\n  margin-bottom: 80px;\n}\n\n#back {\n  margin-bottom: 80px;\n}\n\n#back a {\n  cursor: pointer;\n  width: 44px;\n  display: inline-block;\n}\n\n#top a {\n  cursor: pointer;\n  color: #999;\n  display: inline-block;\n  width: 44px;\n}\n\n.post-category {\n  font-size: 20px;\n}\n\n.post-title {\n  color: #fff;\n  font-weight: bold;\n  font-size: 28px;\n}\n\n.divider {\n  margin-top: 80px;\n  margin-bottom: 80px;\n  width: 100%;\n  height: 1px;\n  border: none;\n  background-color: #ccc;\n}\n\n#post-main {\n  color: #fff;\n  width: 100%;\n  line-height: 2;\n}\n\n#main >>> div.toc a {\n  font-size: 12px;\n  color: #666666;\n  text-decoration: none;\n}\n\n#post-main >>> > * {\n  margin-bottom: 1.5em;\n}\n\n#post-main >>> div.toc + * {\n  margin-top: 0;\n}\n\n#post-main >>> > h2 {\n  font-weight: bold;\n  font-size: 20px;\n  line-height: 1.5;\n  margin-top: 50px;\n  margin-bottom: 21px;\n}\n\n#post-main >>> img {\n  max-width: 100%;\n  height: auto;\n  box-shadow: 0 0 5px #ccc;\n}\n\n#post-main >>> strong {\n  background-color: yellow;\n}\n\n#post-main >>> div.toc {\n  display: none;\n}\n\n#post-main >>> p {\n  text-indent: 1em;\n}\n\n#toc {\n  display: none;\n}\n\n@media (min-width: 768px) {\n  #post-main {\n    width: 650px;\n  }\n}\n\n@media (min-width: 1024px) {\n  #main {\n    display: grid;\n    grid-template-columns: 150px 650px;\n    column-gap: 50px;\n  }\n\n  #toc {\n    grid-column: 1;\n    display: block;\n    align-self: start;\n    position: sticky;\n    top: 20px;\n  }\n\n  #post-main {\n    grid-column: 2;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}